lt {\r\n      name: \"home\",\r\n      components:{\r\n        Navbar,\r\n        TabControl,\r\n        GoodsList,\r\n\r\n        homeSwiper,\r\n        recommendView,\r\n        Feature,\r\n\r\n      },\r\n\r\n\r\n      data(){\r\n        return{\r\n          // 保存好请求回来的数据\r\n          banners:[],\r\n          recommends:[],\r\n          // 建立商品的数据保存结构\r\n          goods:{\r\n            'pop':{page:0,list:[]},\r\n            'new':{page:0,list:[]},\r\n            'sell':{page:0,list:[]}\r\n          }\r\n        }\r\n      },\r\n\r\n\r\n      // 使用创建完成的生命周期函数调用数据请求\r\n      created() {\r\n        // 请求多个参数\r\n        this.getHomeMutidata()\r\n        // 请求商品数据\r\n        this.getHomeGood('pop')\r\n        this.getHomeGood('new')\r\n        this.getHomeGood('sell')\r\n\r\n      },\r\n\r\n      methods:{\r\n        // 拿到多条数据\r\n        getHomeMutidata(){\r\n          getHomeMutidata().then(res => {\r\n            // 把请求的结果保存到一个变量中去\r\n            /*\r\n            this:箭头函数的this 会向上找第一个有this定义的对象，\r\n            而这里上一个有this定义的对象就是当前的组件对象\r\n             */\r\n            this.banners = res.data.banner.list;\r\n            this.recommends = res.data.recommend.list;\r\n          })\r\n        },\r\n\r\n        // 拿到商品数据\r\n        getHomeGood(type){\r\n          const page = this.goods[type].page+1\r\n          getHomeGood(type,page).then(res =>{\r\n            // 这里使用了push方法的可变参数\r\n            this.goods[type].list.push(...res.data.list)\r\n            this.goods[type].page+=1\r\n          })\r\n        }\r\n      }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n #home{\r\n   padding-top: 44px;\r\n }\r\n .tab-control{\r\n   position: sticky;\r\n   top:44px;\r\n    z-index: 9;\r\n }\r\n</style>"],"sourceRoot":"src/views/Home"}]}