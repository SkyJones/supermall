ALA;AA8BA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,KAAA,WAAA,EAAA;AAAA;AACA,aAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAFA;AAGA,OAJA,MAIA;AAAA;AACA,aAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,OAAA,GAAA,IAAA;AACA,SAFA;AAGA;AACA,KAXA;AAYA,IAAA,SAZA,uBAYA;AACA,UAAA,CAAA,KAAA,WAAA,EAAA;AAAA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,UAAA,EAAA,IAAA;AACA;AACA;AAhBA;AA9BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"bottom-bar\">\r\n    <!--左边：全选和选中-->\r\n    <div class=\"check-content\">\r\n      <Check-button\r\n              :is-checked=\"isSelectAll\"\r\n              @click.native=\"checkClick\"\r\n              class=\"button-class\">\r\n      </Check-button>\r\n      <span>全选</span>\r\n    </div>\r\n    <!--中间总价-->\r\n    <div class=\"p\">\r\n      合计：{{totalPrice}}\r\n    </div>\r\n\r\n    <!--右边总数-->\r\n    <div class=\"t\" @click=\"calcClick()\">\r\n      总数（ {{totalLength}} ）\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import CheckButton from \"../../../components/content/CheckButton/CheckButton\";\r\n  import {mapGetters} from 'vuex'\r\n  export default {\r\n    name: \"carBottomBar\",\r\n    components:{\r\n      CheckButton,\r\n    },\r\n    computed:{\r\n      ...mapGetters(['cartList']),\r\n      // 商品总计\r\n      totalPrice(){\r\n        return '￥' + this.$store.state.cartList.filter(item => {\r\n          return item.checked === true\r\n        }).reduce((prevalue,n) =>{\r\n          return prevalue + n.price * n.count\r\n        },0).toFixed(2)\r\n      },\r\n      // 商品数量\r\n      totalLength(){\r\n        return this.$store.state.cartList.filter(item => {\r\n          return item.checked === true\r\n        }).length\r\n      },\r\n      // 全选按钮\r\n      isSelectAll(){\r\n        if (this.cartList.length ===0 ){\r\n            return false\r\n        } else{\r\n            return !this.cartList.find(item=>!item.checked)\r\n        }\r\n      }\r\n    },\r\n    methods:{\r\n      checkClick(){\r\n        if (this.isSelectAll){   //全部选中   ---->改为全部不选中\r\n          this.cartList.forEach(item =>{\r\n            item.checked = false\r\n          })\r\n        }else{               //不是全部选中    ----->改为全部选中\r\n          this.cartList.forEach(item =>{\r\n            item.checked = true\r\n          })\r\n        }\r\n      },\r\n      calcClick(){\r\n        if (!this.isSelectAll){  //全部没有选中\r\n          this.$toast.show('请选择购买的商品',1500)\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n  .bottom-bar{\r\n    display: flex;\r\n    height: 40px;\r\n    width: 100%;\r\n    background-color: #eeeeee;\r\n    position: relative;\r\n  }\r\n  .check-content{\r\n    height: 100%;\r\n    width: 20%;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-evenly;\r\n  }\r\n  .p{\r\n    height: 100%;\r\n    flex: 1;\r\n    margin-left: 20px;\r\n    color: red;\r\n    line-height: 40px;\r\n  }\r\n  .t{\r\n    height: 100%;\r\n    width:30%;\r\n    color:white;\r\n    background-color: rgba(241, 39, 39, 0.99);\r\n    line-height: 40px;\r\n    text-align: center;\r\n  }\r\n</style>"],"sourceRoot":"src/views/Shopcart/shopcartChildren"}]}